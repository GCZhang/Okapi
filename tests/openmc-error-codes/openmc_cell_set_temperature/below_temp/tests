[Tests]
  # temperature below available limit
  [./below_temp]
    type = 'RunException'
    input = master.i
    should_crash = false
    required_applications = 'BuffaloApp'
    expect_err = "OpenMC Error: 'Nuclear data has not been loaded beyond lower bound of T=1.57918 K.' Setting temperature to nearest available point!"
  [../]
[]
